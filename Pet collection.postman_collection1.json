	"item": [
		{
			"name": "Create Pet",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"const json = pm.response.json();",
							"pm.test(\"Pet id matches\", () => json.id === Number(pm.collectionVariables.get(\"pet_id\")));",
							"pm.collectionVariables.set(\"pet_id\", json.id);",
							"",
							"pm.test(\"Correct pet name\", () => {",
							"  pm.expect(Number(json.name)).to.eql(Number(pm.collectionVariables.get(\"pet_name\")));",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"const unique = Date.now().toString() + Math.floor(Math.random() * 1000);",
							"",
							"const petId = Number(unique.slice(-12));",
							"",
							"pm.collectionVariables.set(\"pet_id\", petId);",
							"",
							"pm.collectionVariables.set(\"pet_name\", `pet-${pm.collectionVariables.get(\"pet_id\")}`);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
